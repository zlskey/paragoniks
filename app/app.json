{
  "expo": {
    "name": "Paragoniks",
    "slug": "paragoniks",
    "version": "1.2.27",
    "orientation": "portrait",
    "icon": "./src/assets/images/splash.png",
    "scheme": "paragoniks",
    "newArchEnabled": true,
    "platforms": [
      "android",
      "web"
    ],
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#0F0F0F",
      "image": "./src/assets/images/splash.png",
      "resizeMode": "cover"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#0F0F0F"
      },
      "package": "com.zlskey.paragoniks",
      "googleServicesFile": "./google-services.json",
      "versionCode": 35
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-font",
      "expo-asset",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./src/assets/images/adaptive-icon.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "eas": {
        "projectId": "adfc708a-b990-4549-b269-3bf1fbca4e8f"
      }
    },
    "owner": "zlskey"
  }
}
